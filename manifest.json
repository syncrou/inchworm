 {
  "manifest_version": 3,
  "name": "QA Screenshot Tester",
  "version": "1.0",
  "description": "Automates QA tests by comparing screenshots.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}